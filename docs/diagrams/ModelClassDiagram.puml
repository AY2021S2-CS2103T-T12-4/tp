@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyEndpointList <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class EndpointList
Class ReadOnlyEndpointList
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Endpoint {
Class Endpoint
Class Address
Class Data
Class Method
Class UniqueEndpointList
}

Package Header {
Class Header
}

Package Tag {
Class Tag
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

EndpointList .up.|> ReadOnlyEndpointList

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" EndpointList
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

EndpointList *--> "1" UniqueEndpointList
UniqueEndpointList o--> "*" Endpoint
Endpoint *--> "1" Method
Endpoint *--> "1" Data
Endpoint *--> "1" Address
Endpoint *--> "*" Header
Endpoint *--> "*" Tag

Method -[hidden]right-> Address
Address -[hidden]right-> Data

ModelManager ----->" ~* filtered list" Endpoint
@enduml
